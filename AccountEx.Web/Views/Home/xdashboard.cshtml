@{
    ViewBag.Title = "Dashboard";
    ViewBag.TitleDescription = "dashboard & statistics";
}

@section PageTitle
{

    <h1 class="font-green-sharp bold">
        Bussiness Summary
    </h1>

}
<div id="tile-container" class="row"></div>
<div class="row hide">
    <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
        <div class="dashboard-stat2 ">
            <div class="display">
                <div class="number">
                    <h3 class="font-green-sharp">
                        <span data-counter="counterup" data-value="7800">0</span>
                        <small class="font-green-sharp">PKR</small>
                    </h3>
                    <small>TOTAL Sales</small>
                </div>
                <div class="icon">
                    <i class="icon-pie-chart"></i>
                </div>
            </div>
            <div class="progress-info">
                <div class="progress">
                    <span style="width: 76%;" class="progress-bar progress-bar-success green-sharp">
                        <span class="sr-only">76% progress</span>
                    </span>
                </div>
                <div class="status">
                    <div class="status-title">progress </div>
                    <div class="status-number">76% </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
        <div class="dashboard-stat2 ">
            <div class="display">
                <div class="number">
                    <h3 class="font-red-haze">
                        <span data-counter="counterup" data-value="1349">0</span>
                    </h3>
                    <small>Receivables</small>
                </div>
                <div class="icon">
                    <i class="icon-like"></i>
                </div>
            </div>
            <div class="progress-info">
                <div class="progress">
                    <span style="width: 85%;" class="progress-bar progress-bar-success red-haze">
                        <span class="sr-only">85% change</span>
                    </span>
                </div>
                <div class="status">
                    <div class="status-title">change </div>
                    <div class="status-number">85% </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
        <div class="dashboard-stat2 ">
            <div class="display">
                <div class="number">
                    <h3 class="font-blue-sharp">
                        <span data-counter="counterup" data-value="567"></span>
                    </h3>
                    <small>Payables</small>
                </div>
                <div class="icon">
                    <i class="icon-basket"></i>
                </div>
            </div>
            <div class="progress-info">
                <div class="progress">
                    <span style="width: 45%;" class="progress-bar progress-bar-success blue-sharp">
                        <span class="sr-only">45% grow</span>
                    </span>
                </div>
                <div class="status">
                    <div class="status-title">grow </div>
                    <div class="status-number">45% </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
        <div class="dashboard-stat2 ">
            <div class="display">
                <div class="number">
                    <h3 class="font-purple-soft">
                        <span data-counter="counterup" data-value="276"></span>
                    </h3>
                    <small>Cash & Bank</small>
                </div>
                <div class="icon">
                    <i class="icon-user"></i>
                </div>
            </div>
            <div class="progress-info">
                <div class="progress">
                    <span style="width: 57%;" class="progress-bar progress-bar-success purple-soft">
                        <span class="sr-only">56% change</span>
                    </span>
                </div>
                <div class="status">
                    <div class="status-title">change </div>
                    <div class="status-number">57% </div>
                </div>
            </div>
        </div>
    </div>
</div>





<div id="widget-container"></div>
<div id="jsondata" class="hide">
    <p id="data">@ViewBag.FormSetting</p>
</div>
<script type="text/template" id="template-tile">
    <div class="col-lg-2 col-md-2 col-sm-6 col-xs-12">
        <div class="dashboard-stat2 ">
            <div class="display">
                <div class="number">
                    <h3 class="font-[Color]">
                        [PStart]<span data-counter="counterup" data-value="[Balance]">0</span>[PEnd]
                        <small class="hide font-[Color]">PKR</small>
                    </h3>
                    <small>[Label]</small>
                </div>

            </div>
            <div class="progress-info">
                <div class="progress">
                    <span style="width: 76%;" class="progress-bar progress-bar-success [Color]">
                        <span class="sr-only">76% progress</span>
                    </span>
                </div>
                <div class="status">
                    <div class="status-title">progress </div>
                    <div class="status-number">76% </div>
                </div>
            </div>
        </div>
    </div>


</script>

<script type="text/template" id="template-default">
    <div class="col-md-6 col-sm-6">
        <div class="portlet light ">
            <div class="portlet-title tabbable-line">
                <div class="caption font-green">
                    <span class="caption-subject bold uppercase">[Name]</span>
                </div>
                <ul class="nav nav-tabs">
                    <li class="active">
                        <a href="#tab1_[Id]" class="active" data-toggle="tab">Graph </a>
                    </li>
                    <li>
                        <a href="#tab2_[Id]" data-toggle="tab"> Data</a>
                    </li>
                </ul>
            </div>
            <div class="portlet-body">
                <div class="tab-content">
                    <div class="tab-pane active" id="tab1_[Id]">
                        <div class="scroller data-conatiner" data-always-visible="1" data-rail-visible="0">
                            <div id="echarts_bar_[Id]" style="height:350px;"></div>


                        </div>
                    </div>
                    <div class="tab-pane" id="tab2_[Id]">

                        <div class="scroller12 graph-conatiner" data-always-visible="1" data-rail-visible1="1" style="height:350px;">
                            <table class='table table-striped table-bordered table-hover report-table'>
                                <thead>
                                    [TableHead]
                                </thead>
                                <tbody>
                                    [TableBody]
                                </tbody>
                                <tfoot>
                                    [TableFooter]
                                </tfoot>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


</script>
