@{
    ViewBag.FormTitle = "Voucher";
    var type = Request.QueryString["type"] + "";
    switch (type.ToLower())
    {
        case "sale":
        case "sales":
            ViewBag.FormTitle = "Sale";
            break;
        case "purchase":
        case "purchases":
            ViewBag.FormTitle = "Purchase";
            break;
        case "salereturn":
            ViewBag.FormTitle = "Sale Return";
            break;
        case "purchasereturn":
            ViewBag.FormTitle = "Purchase Return";
            break;
    }
    ViewBag.Title = ViewBag.FormTitle;
}
