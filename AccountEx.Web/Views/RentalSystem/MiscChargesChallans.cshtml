@using AccountEx.Common
@using AccountEx.BussinessLogic;

@{ 
    ViewBag.Title = "Miscellaneous Charges Challans";
}
@{ 
    var years = Enumerable.Range(DateTime.Now.Year - 5, 15).ToList();
    var monthslist = new List<IdName>();
    var months = System.Globalization.CultureInfo.CurrentCulture.DateTimeFormat.MonthNames;
    for (int i = 0; i < months.Length; i++)
    {
        if (i <= 11)
        {
            monthslist.Add(new IdName() { Name = months[i], Id = i + 1 });
        }

    }
    var currentmonth = DateTime.Now.Month;
}

@section PageTitle
{

    <h1 class="font-green-sharp bold hidden-print">Craete Misc Challans</h1>
}
<!-- END PAGE HEADER-->
<div class="row hidden-print" id="form-info">
    <div class="col-md-12 col-sm-12">
        <!-- BEGIN EXAMPLE TABLE PORTLET-->
        <div class="portlet light">
            <div class="portlet-title hide">
                <div class="caption">
                    <i class="fa fa-edit"></i>Add/Edit Challans
                </div>


                <div class="tools hide">
                    <a href="javascript:;" class="collapse"></a>
                </div>
            </div>
            <div class="portlet-body form">
                <div class="form-horizontal" data-save="save">

                    <div class="form-body">


                        <div class="row">
                            <div class="col-md-1"></div>
                            <div class="col-md-8">
                                <div class="form-group">
                                    <label class="col-md-2 control-label" for="prefix">Tenants</label>
                                    <div class="col-md-10">
                                        <select id="RentAgreementId" class="form-control select2" data-required="required" data-message="Tenant is required">
                                        </select>
                                    </div>

                                </div>
                                <div class="form-group">
                                    <label class="col-md-2 control-label" for="prefix">Charge</label>
                                    <div class="col-md-10">
                                        <select id="ChargeId" class="form-control select2" data-required="required" data-message="Charge is required">
                                            <option></option>
                                            @foreach (var p in (List<IdName>)ViewBag.Charges)
                                            {
                                                <option data-custom="@p.Id" value="@p.Id">@p.Name</option>
                                            }
                                        </select>
                                    </div>

                                </div>
                                <div class="form-group">
                                    <label class="col-md-2 control-label" for="prefix">Month</label>
                                    <div class="col-md-3">
                                        <select id="Month" class="form-control select2" data-required="required" data-message="Month is required.">
                                            <option></option>
                                            @foreach (var item in monthslist)
                                            {
                                                <option value="@item.Id">@item.Name</option>
                                            }
                                        </select>

                                    </div>
                                    <label class="col-md-2 control-label" for="prefix">Year</label>
                                    <div class="col-md-2">
                                        <select id="Year" class="form-control select2" data-required="required" data-message="year is required.">
                                            <option></option>
                                            @foreach (var year in years)
                                            { 
                                                <option>@year</option>
                                            }
                                        </select>

                                    </div>

                                    <label class="col-md-1 control-label" for="prefix">Amount</label>
                                    <div class="col-md-2">
                                        <input type="hidden" id="EntryType" value="@((byte)EntryType.Manual)" data-clear="false" />
                                        <input type="hidden" id="TransactionType" value="@((byte)VoucherType.MiscCharges)" data-clear="false" />
                                        <input type="hidden" id="AmountPerInstallment" />
                                        <input type="text" class="form-control input-xsmall num3" id="NetAmount" data-plus-as-tab="false" data-required="required" data-message="amount is required" />
                                    </div>

                                </div>

                                <div class="form-group">

                                    <label class="col-md-2 control-label" for="prefix">Due Date</label>
                                    <div class="col-md-3">
                                        <input id="DueDate" type="text" class="form-control date-picker" data-required="required" data-message="Due date is required" data-plus-as-tab="false" data-clear-type="date" data-fiscal-date="true">
                                    </div>
                                    <label class="col-md-2 control-label" for="prefix">Fined Person</label>
                                    <div class="col-md-5">
                                        <input type="text" class="form-control" id="FinedPerson" />
                                    </div>
                                </div>

                                <div class="form-group">

                                    <label class="col-md-2 control-label" for="prefix">Remarks</label>
                                    <div class="col-md-10">
                                        <input type="text" class="form-control" id="Remarks" />
                                    </div>
                                </div>
                                <div class="form-group">
                                </div>



                                <div class="btn-set pull-right">
                                    @if (!SiteContext.Current.Fiscal.IsClosed)
                                    {
                                        <button type="button" class="btn purple-plum" onclick="MiscChargesChallans.New()">
                                            <i class="fa  fa-plus-circle"></i>
                                            New</button>
                                        <button type="button" class="btn btn-primary green" onclick="MiscChargesChallans.Save()">
                                            <i class="fa fa-check"></i>
                                            Create Chalaln</button>
                               
                                    }

                                    <button type="button" class="btn blue" onclick="window.location='@(Url.Content(SettingManager.DashBoardUrl))'">
                                        <i class="fa  fa-times-circle"></i>

                                        Close</button>
                                </div>

                            </div>

                        </div>

                        <div class="row">
                            <div class="col-md-1"></div>

                            <div class="col-md-8 hide" id="div-table">
                                <div class="portlet box light">
                                    <div class="portlet-title">
                                        <div class="caption">
                                            <i class="fa fa-list"></i>Existing Challans
                                        </div>

                                        <div class="tools">
                                            <a href="javascript:;" class="collapse"></a>
                                        </div>
                                    </div>

                                    <div class="portlet-body">
                                        <div class="hide alert alert-danger container-message">
                                            <i class="close" onclick="$(this).parent().hide();"></i>
                                            <strong></strong>
                                            <div class="form-body message-content"></div>
                                        </div>
                                        <table class="table table-striped table-bordered table-hover" id="mainTable">
                                            <thead>
                                                <tr>

                                                    <th>Challan No.</th>
                                                    <th>Due Date</th>
                                                    <th>Total Amount</th>
                                                    <th>Status</th>

                                                    <th style="width: 150px;" class="no-sort"></th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr>
                                                    <td colspan="5"></td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>



                </div>

            </div>
        </div>
        <!-- END EXAMPLE TABLE PORTLET-->
    </div>
</div>


<div id="jsondata" class="hide">
    <input id="FormSetting"  type="text" value="@ViewBag.FormSetting" />
