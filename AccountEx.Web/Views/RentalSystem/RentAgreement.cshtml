@using AccountEx.Common;
@using AccountEx.BussinessLogic;
@{
    var years = Enumerable.Range(DateTime.Now.Year - 5, 15).ToList();
    ViewBag.FormTitle = "Rent Agreement";
    ViewBag.Title = ViewBag.FormTitle;
    var monthslist = new List<IdName>();
    var months = System.Globalization.CultureInfo.CurrentCulture.DateTimeFormat.MonthNames;
    for (int i = 0; i < months.Length; i++)
    {
        if (i <= 11)
        {
            monthslist.Add(new IdName() { Name = months[i], Id = i + 1 });
        }

    }
}

<style>
    table tfoot tr td {
        font-weight: 400 !important;
    }
</style>

@section PageTitle
{

    <h1 class="font-green-sharp bold">Manage Rent Agreement</h1>
}
<!-- END PAGE HEADER-->
<div class="row" id="form-info">
    <div class="col-md-12 col-sm-12">
        <!-- BEGIN EXAMPLE TABLE PORTLET-->
        <div class="portlet light">
            <div class="portlet-title">
                <div class="caption">
                    <i class="fa fa-edit"></i>Add/Edit Rent Agreement
                </div>
                <div class="actions">
                    <a href="javascript:;" onclick="RentAgreement.ListView()" class="btn blue">
                        <i class="fa fa-angle-left"></i>&nbsp;Back to list
                    </a>

                    <button id="btn-refresh-coa" type="button" class="btn red btn-sm">
                        <i class="fa fa-refresh"></i>
                        Refresh Accounts
                    </button>
                </div>
                <div class="tools hide">
                    <a href="javascript:;" class="collapse"></a>
                </div>
            </div>
            <div class="portlet-body form">
                <div class="form-horizontal" data-save="save">
                    <input type="text" id="Id" class="hide" data-include-in-item="true" data-item-id="RentAgreementId" />
                    <input type="text" id="TenantAccountId" class="hide" />
                    <input type="text" id="ShopId" class="hide" />
                    <input type="text" id="VoucherNumber" class="hide" />
                    <input type="text" id="TempCode" class="hide" />
                    <input type="text" id="IsRenew" class="hide" />
                    <div class="form-body">
                        <div class="hide alert alert-danger container-message">
                            <i class="close" onclick="$(this).parent().hide();"></i>
                            <strong></strong>
                            <div class="form-body message-content"></div>
                        </div>

                        <div class="row">
                            <div class="col-md-6">
                                <div class="portlet">
                                    <div class="portlet-title">
                                        <div class="caption">
                                            Tenant Info
                                        </div>
                                    </div>
                                </div>

                                <div class="form-group">

                                    <label class="col-md-4 control-label" for="prefix">Account Code</label>
                                    <div class="col-md-6">
                                        <input type="text" class="form-control " id="AccountCode" data-required="required" data-message="Account Code is required" data-plus-as-tab="false" />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-4 control-label" for="prefix">Tenant Name</label>
                                    <div class="col-md-6">
                                        <input type="text" class="form-control" id="TenantName" readonly="readonly" />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-4 control-label" for="prefix">Business</label>
                                    <div class="col-md-6">
                                        <input type="text" class="form-control " id="Business" data-plus-as-tab="false" readonly="readonly" />
                                    </div>

                                </div>
                                <div class="form-group">
                                    <label class="col-md-4 control-label" for="prefix">Brand</label>
                                    <div class="col-md-6">
                                        <input type="text" class="form-control " id="Brand" data-plus-as-tab="false" readonly="readonly" />
                                    </div>

                                </div>

                                <div class="form-group">
                                    <label class="col-md-4 control-label" for="prefix">No. of Partners</label>
                                    <div class="col-md-6">
                                        <input class="form-control number" type="text" name="text" id="NumberOfPartners" readonly="readonly" />
                                    </div>
                                </div>

                                <div class="form-group hide">
                                    <label class="col-md-4 control-label" for="prefix">Contact No</label>
                                    <div class="col-md-6">
                                        <input type="text" class="form-control " id="ContactNo" data-plus-as-tab="false" readonly="readonly" />
                                    </div>

                                </div>

                                <div class="form-group hide">
                                    <label class="col-md-4 control-label" for="prefix">Single</label>
                                    <div class="col-md-6">
                                        <input disabled class="form-control number" type="text" name="text" id="Single" readonly="readonly" />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-4 control-label" for="prefix">Partnership</label>
                                    <div class="col-md-6">
                                        <input disabled class="form-control" type="text" name="text" id="Partnership" readonly="readonly" />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-4 control-label" for="prefix">&nbsp;</label>
                                    <div class="col-md-3">
                                        <label id="LableActive" class="control-label" for="IsActive">
                                            <input type="checkbox" id="IsActive" data-checktrack='true' />
                                            Active
                                        </label>
                                    </div>


                                </div>
                            </div>

                            <div class="col-md-6">
                                <div class="portlet">
                                    <div class="portlet-title">
                                        <div class="caption">
                                            Shop Info
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-4 control-label" for="prefix">Shop No</label>
                                    <div class="col-md-6">
                                        <input type="text" class="form-control " id="ShopNo" data-required="required" data-message="Shop No is required" data-plus-as-tab="false" />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-4 control-label" for="prefix">Block</label>
                                    <div class="col-md-6">
                                        <input type="text" class="form-control " id="Block" readonly="readonly" data-plus-as-tab="false" />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-4 control-label" for="prefix">Area(sqft)</label>
                                    <div class="col-md-6">
                                        <input type="text" class="form-control" readonly="readonly" id="Area" />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-4 control-label" for="prefix">North</label>
                                    <div class="col-md-6">
                                        <input disabled class="form-control number" readonly="readonly" type="text" name="text" id="North" />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-4 control-label" for="prefix">South</label>
                                    <div class="col-md-6">
                                        <input class="form-control" type="text" name="text" id="South" readonly="readonly" />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-4 control-label" for="prefix">East</label>
                                    <div class="col-md-6">
                                        <input class="form-control number" type="text" name="text" id="East" readonly="readonly" />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-4 control-label" for="prefix">West</label>
                                    <div class="col-md-6">
                                        <input disabled class="form-control" type="text" name="text" id="West" readonly="readonly" />
                                    </div>
                                </div>

                            </div>
                        </div>
                        <div class="row hide">
                            <div class="col-md-6">
                                <div class="portlet">
                                    <div class="portlet-title">
                                        <div class="caption">
                                            Agreement Info
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-4 control-label" for="prefix">Start Date</label>
                                    <div class="col-md-6">
                                        <input type="text" class="date-picker form-control" id="StartDate" />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-4 control-label" for="prefix">End Date</label>
                                    <div class="col-md-6">
                                        <input type="text" disabled class="date-picker form-control" id="EndDate" />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-4 control-label" for="prefix">Agreement PD</label>
                                    <div class="col-md-6">
                                        <input type="text" class="form-control" id="AgreementPD" />
                                    </div>

                                </div>
                                <div class="form-group">
                                    <label class="col-md-4 control-label" for="prefix">Yearly Inc.</label>
                                    <div class="col-md-6">
                                        <input class="form-control" type="text" name="text" id="YearlyInc" value="10" />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-4 control-label" for="prefix">Rent/Sqft</label>
                                    <div class="col-md-6">
                                        <input class="form-control" type="text" name="text" id="RentPerSqft" value="@((int)SettingManager.RentPerSqft)" />
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-6">
                                <div class="portlet">
                                    <div class="portlet-title">
                                        <div class="caption">
                                            Installments Info
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-4 control-label" for="prefix">Rent No of Installments</label>
                                    <div class="col-md-6">
                                        <input type="text" class=" form-control" id="RentInstallments" />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-4 control-label" for="prefix">Possession No of Installments</label>
                                    <div class="col-md-6">
                                        <input type="text" class="form-control" id="PossessionInstallments" />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-4 control-label" for="prefix">Security Money No of Installments</label>
                                    <div class="col-md-6">
                                        <input type="text" class="form-control" id="SecurityMoneyInstallments" />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-4 control-label" for="prefix">Misc Changes No of Installments</label>
                                    <div class="col-md-6">
                                        <input type="text" class="form-control" id="MiscChangesInstallments" />
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row" data-save="false">
                            <div class="col-md-1"></div>
                            <div class="col-md-11 scroller12" data-always-visible="1" data-rail-visible1="1">
                                <div class="portlet">
                                    <div class="portlet-title">
                                        <div class="caption">
                                            <i class="fa fa-list"></i>Rent Schedule

                                        </div>

                                    </div>
                                </div>
                                <input type="hidden" id="LastRentMonth" />
                                <input type="hidden" id="LastRentYear" />
                                <a id="RemoveUnpaidMonths" class="edit-visible hide" style="float: right; margin-top: -20px;" onclick="RentAgreement.RemoveUnpaidMonths();">Remove unpaid months</a>

                                <table id="item-container" class="table table-bordered griditem">
                                    <thead>
                                        <tr>
                                            <th class='align-center'>Parts</th>
                                            <th>From</th>
                                            <th>To</th>
                                            <th class='align-right'>Area</th>
                                            <th class='align-right'>Rate</th>
                                            <th style="width: 20%;">Remarks</th>
                                            <th class='align-right'>Inc. %</th>
                                            <th class='align-right'>Inc. Amount</th>
                                            <th class='align-right'>Net Rent</th>
                                            <th class='align-right'>UC %</th>
                                            <th class='align-right'>UC Amount</th>
                                            <th class='align-right'>Total</th>
                                            <th></th>
                                        </tr>
                                    </thead>
                                    <tbody data-plus-as-tab="false"></tbody>
                                    <tfoot class="hide">
                                        <tr>
                                            <td colspan="2">Rent Arrears</td>
                                            <td colspan="3">
                                                <span style="display: inline-block">
                                                    <select id="ArrearMonth">
                                                        <option>Month</option>
                                                        @foreach (var item in monthslist)
                                                        {
                                                            <option value="@item.Id">@item.Name</option>
                                                        }
                                                    </select>
                                                    <span>&nbsp &nbsp &nbsp &nbsp </span>
                                                    <select id="ArrearYear">
                                                        <option>Year</option>
                                                        @foreach (var year in years)
                                                        {
                                                            <option value="@year">@year</option>
                                                        }
                                                    </select>
                                                </span>
                                            </td>
                                            <td colspan="2"></td>
                                            <td>
                                                <input type="text" class="ArrearAmount form-control" id="ArrearAmount">
                                            </td>
                                        </tr>
                                    </tfoot>
                                </table>
                            </div>
                        </div>

                        <!--for security charges-->
                        <div class="row">
                            <div class="col-md-2"></div>
                            <div class="col-md-8 scroller12" data-always-visible="1" data-rail-visible1="1">
                                <div class="portlet">
                                    <div class="portlet-title">
                                        <div class="caption">
                                            <i class="fa fa-list"></i>Security Charges Schedule (12 Month Rent + 20% UC)
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-3 control-label" for="prefix">Security Money Amount</label>
                                    <div class="col-md-3">
                                        <input type="text" class="form-control edit-disabled12" id="SecurityMoneyAmount" />
                                        <input type="text" class="hide SecurityMoneyAmount" />
                                    </div>
                                    <div class="col-md-6 edit-visible hide">
                                        <label>No. of Installments received:  <b id="SecurityInstallmentsReceived"></b></label>
                                    </div>


                                </div>
                                <div class="form-group">
                                    <label class="col-md-3 control-label" for="prefix">Received Amount</label>
                                    <div class="col-md-3">
                                        <input type="text" class="form-control edit-disabled12" id="ReceivedSecurityAmount" />
                                    </div>
                                    <div class="col-md-6 edit-visible hide">
                                        <label>Amount received by Installments: <b id="SecurityAmountReceived"></b></label>
                                    </div>


                                </div>
                                <div class="form-group">
                                    <label class="col-md-3 control-label" for="prefix">Balance To Be Paid</label>
                                    <div class="col-md-3">
                                        <input type="text" class="form-control" id="SecurityBalance" />
                                    </div>

                                    <div class="col-md-2 edit-visible12 hide">
                                        <label>Adjustment Type</label>
                                    </div>
                                    <div class="col-md-3 edit-visible12 hide">
                                        <select id="ExtraSecurityType" class="form-control input-xsmall pull-left" data-placeholder="Adjustment Type">
                                            <option value="2">DR</option>
                                            <option value="1">CR</option>
                                        </select>&nbsp;
                                        &nbsp;<input type="text" class="form-control input-xsmall pull-right" id="ExtraSecurityAmount" />&nbsp;


                                    </div>

                                </div>
                                <table id="SecurityCharges" class="table table-bordered griditem">
                                    <thead>
                                        <tr>
                                            <th>No of Installment</th>
                                            <th>Per Installment</th>
                                            <th>Round Amount</th>
                                        </tr>
                                    </thead>
                                    <tbody data-plus-as-tab="false">
                                        <tr>
                                            <td>
                                                <input type='text' class='form-control SecurityInstallment' id="SecurityInstallment" />
                                                <input type='text' class='hide ExistingSecurityInstallment' />


                                            </td>
                                            <td>
                                                <input type='text' readonly="readonly" class='form-control SecurityPerInstallment' id="SecurityPerInstallment">
                                            </td>
                                            <td>
                                                <input type='text' readonly="readonly" class='SecurityRoundAmount' id="SecurityRoundAmount">
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>

                        <!--for possession charges-->
                        <div class="row">
                            <div class="col-md-2"></div>
                            <div class="col-md-8 scroller12" data-always-visible="1" data-rail-visible1="1">
                                <div class="portlet">
                                    <div class="portlet-title">
                                        <div class="caption">
                                            <i class="fa fa-list"></i>Possession Charges Schedule
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-3 control-label" for="prefix">Already Paid</label>
                                    <div class="col-md-3">
                                        <input type="text" class="form-control edit-disabled12" id="AlreadyPaidPossessionAmount" />
                                    </div>
                                    <div class="col-md-6 edit-visible hide">
                                        <label>No. of Installments received:  <b id="PossessionInstallmentsReceived"></b></label>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-3 control-label" for="prefix">Not Paid Amount</label>
                                    <div class="col-md-3">
                                        <input type="text" class="form-control edit-disabled12" id="NotPaidPossessionAmount" />
                                    </div>
                                    <div class="col-md-6 edit-visible hide">
                                        <label>Amount received by Installments: <b id="lblPossessionAmountReceived"></b></label>
                                        <input type="text" class="form-control edit-disabled hide" id="PossessionAmountReceived" />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-3 control-label" for="prefix">Total Amount</label>
                                    <div class="col-md-3">
                                        <input type="text" class="form-control edit-disabled" id="TotalPossessionAmount" />
                                        <input type="text" class="hide TotalPossessionAmount">
                                    </div>
                                    <div class="col-md-2 edit-visible12 hide">
                                        <label>Adjustment Type</label>
                                    </div>
                                    <div class="col-md-3 edit-visible12 hide">
                                        <select id="ExtraPossessionType" class="form-control input-xsmall pull-left" data-placeholder="Adjustment Type">

                                            <option value="2">DR</option>
                                            <option value="1">CR</option>


                                        </select>&nbsp;
                                        &nbsp;<input type="text" class="form-control input-xsmall pull-right" id="ExtraPossessionAmount" />&nbsp;
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-3 control-label" for="prefix">Received</label>
                                    <div class="col-md-3">
                                        <input type="text" class="form-control edit-disabled12" id="PossessionReceived" />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-3 control-label" for="prefix">Balance</label>
                                    <div class="col-md-3">
                                        <input type="text" class="form-control" id="PossessionBalance" />
                                    </div>
                                </div>

                                <table id="PossessionCharges" class="table table-bordered griditem">
                                    <thead>
                                        <tr>
                                            <th>No of Installment</th>
                                            <th>Per Installment</th>
                                        </tr>
                                    </thead>
                                    <tbody data-plus-as-tab="false">
                                        <tr>
                                            <td>
                                                <input type='text' class='PossessionInstallment form-control' id="PossessionInstallment">
                                                <input type='text' class="hide ExistingPossessionInstallment">
                                            </td>
                                            <td>
                                                <input type='text' readonly="readonly" class='PossessionPerInstallment' id="PossessionPerInstallment">
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>

                    </div>
                    <div class="form-actions">
                        <div class="btn-set pull-left">
                            <a href="javascript:;" onclick="RentAgreement.LoadVoucher('first')" class="btn blue tooltips disabled first" data-original-title="First">
                                <i class="fa fa-angle-double-left"></i>
                            </a>
                            <a href="javascript:;" onclick="RentAgreement.LoadVoucher('previous')" class="btn blue tooltips disabled previous" data-original-title="Previous">
                                <i class="fa fa-angle-left"></i>
                            </a>
                            <a href="javascript:;" onclick="RentAgreement.LoadVoucher('next')" class="btn blue tooltips disabled next" data-original-title="Next">
                                <i class="fa fa-angle-right"></i>
                            </a>
                            <a href="javascript:;" onclick="RentAgreement.LoadVoucher('last')" class="btn blue tooltips disabled last" data-original-title="Last">
                                <i class="fa fa-angle-double-right"></i>
                            </a>
                        </div>
                        <div class="btn-set pull-right">
                            @if (!SiteContext.Current.Fiscal.IsClosed)
                            {
                                <button type="button" class="btn purple-plum" onclick="RentAgreement.New()">
                                    <i class="fa  fa-plus-circle"></i>
                                    New
                                </button>
                                <button type="button" class="btn green hide edit-visible transfer-info" onclick="RentAgreement.GetTransferInfo()">
                                    <i class="fa  fa-plus-circle"></i>
                                    Transfer
                                </button>

                                <button type="button" class="btn purple hide edit-visible" onclick="RentAgreement.Renew()">
                                    <i class="fa  fa-plus-circle"></i>
                                    Renew
                                </button>

                                    <button id="savebutton" type="button" class="btn btn-primary green" onclick="RentAgreement.Save()">
                                        <i class="fa fa-check"></i>
                                        Save
                                    </button>
                                    <button id="btndelete" type="button" class="btn red" onclick="RentAgreement.Delete()">
                                        <i class="fa fa-trash-o"></i>
                                        Delete
                                    </button>
                            }
                            <button id="btnprint" type="button" class="hide btn yellow" onclick="RentAgreement.Print()">
                                <i class="fa fa-print"></i>

                                Print
                            </button>
                            <button type="button" class="btn blue" onclick="window.location='@(Url.Content(SettingManager.DashBoardUrl))'">
                                <i class="fa  fa-times-circle"></i>

                                Close
                            </button>
                        </div>

                    </div>


                </div>

            </div>
        </div>
        <!-- END EXAMPLE TABLE PORTLET-->
    </div>
</div>

<div class="row hidden-print hide" id="div-table">
    <div class="col-md-12 col-sm-12">
        <!-- BEGIN EXAMPLE TABLE PORTLET-->
        <div class="portlet light">
            <div class="portlet-title">
                <div class="caption">
                    <i class="fa fa-list"></i>Rent Agreements
                </div>

                <div class="actions">
                    <button id="btnEdit" type="button" class="btn blue" onclick="RentAgreement.New()"><i class="fa fa-plus"></i>&nbsp;New</button>



                </div>
                <div class="tools hide">
                    <a href="javascript:;" class="collapse"></a>
                </div>
            </div>
            <div class="portlet-body">
                <div class="hide alert alert-danger container-message">
                    <i class="close" onclick="$(this).parent().hide();"></i>
                    <strong></strong>
                    <div class="form-body message-content"></div>
                </div>
                <table class="table table-striped table-bordered table-hover" id="mainTable">
                    <thead>
                        <tr>
                            <th>Tenant Name</th>
                            <th>Shop No</th>
                            <th>Block</th>
                            <th>Start Date</th>
                            <th>End Date</th>

                            <th style="width: 75px;" class="no-sort"></th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td colspan="7">Loading...</td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
        <!-- END EXAMPLE TABLE PORTLET-->
    </div>

</div>
<div id="model-transfer" class="modal fade" tabindex="-1">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                <h4 class="modal-title">
                    <strong>Transfer Detail</strong><span class="Manufacturer"></span>&nbsp;<span class="Model"></span>
                </h4>

            </div>
            <div class="modal-body" data-save="save">




                <div class="row">
                    <input type="text" id="Id" class="hide" data-include-in-item="true" data-item-id="RentAgreementId">
                    <div class="col-md-6">
