@using AccountEx.Common
@using AccountEx.BussinessLogic;

@{
    var type = Request.QueryString["type"] + "";

    switch (type.ToLower())
    {
        case "possessioncharges":
            ViewBag.FormTitle = "Possession Challan";
            break;
        case "securitymoney":
            ViewBag.FormTitle = "Security Money Challan";
            break;
    }
    ViewBag.Title = ViewBag.FormTitle;
}
@section PageTitle
{
    <h1 class="font-green-sharp bold hidden-print">Create @ViewBag.FormTitle</h1>
}
<style>
    .tbl-net-billing td, .tbl-net-billing th, table.tbl-net-billing {
        border-color: #AAAAAA !important;
    }

    .tbl-net-billing tbody tr.tr-net-billing td, .tbl-net-billing tbody tr.tr-balance td {
        background-color: #E9E8E7;
        padding-right: 5px;
    }

    .tbl-net-billing thead tr th {
        font-size: 14px;
    }

    .tbl-net-billing tbody td input {
        padding: 0;
        border-radius: 0;
        border-width: 0;
    }

    .tbl-net-billing tbody td:first-child {
        font-weight: bold;
        height: 24px;
        padding-left: 5px !important;
    }

    .tbl-net-billing tbody td:first-child, .tbl-net-billing thead tr th {
        background-color: #DDD !important;
    }

    .tbl-net-billing, .tbl-net-billing tr th:not(.align-center) {
        text-align: right;
    }

