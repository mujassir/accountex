<div class="row hidden-print" id="form-info">
    <div class="col-md-12 col-sm-12">
        <!-- BEGIN EXAMPLE TABLE PORTLET-->
        <div class="portlet light">
            <div class="portlet-title hide">
                <div class="caption">
                    <i class="fa fa-edit"></i>Add/Edit Challans
                </div>


                <div class="tools hide">
                    <a href="javascript:;" class="collapse"></a>
                </div>
            </div>
            <div class="portlet-body form">
                <div class="form-horizontal" data-save="save">

                    <div class="form-body">


                        <div class="row">
                            <div class="col-md-1"></div>
                            <div class="col-md-8">
                                <div class="form-group">
                                    <label class="col-md-2 control-label" for="prefix">Tenants</label>
                                    <div class="col-md-10">
                                        <select id="RentAgreementId" class="form-control select2" data-required="required" data-message="Tenant is required"></select>
                                    </div>

                                </div>

                                <div class="form-group">
                                    <label class="col-md-2 control-label" for="prefix">Installment</label>
                                    <div class="col-md-2">
                                        <input type="text" class="form-control input-xsmall num5" id="NumberOfInstallment" data-required="required" data-message="No of Months is required" data-plus-as-tab="false" />
                                    </div>
                                    <label class="col-md-1 control-label" for="prefix">Amount</label>
                                    <div class="col-md-1">
                                        <input type="hidden" id="EntryType" value="@((byte)EntryType.Manual)" data-clear="false" />
                                        <input type="hidden" id="AmountPerInstallment" />
                                        <input disabled type="text" class="form-control input-xsmall" id="NetAmount" data-plus-as-tab="false" />
                                    </div>
                                    <label class="col-md-3 control-label" for="prefix">Due Date</label>
                                    <div class="col-md-2">
                                        <input id="DueDate" type="text" class="form-control date-picker input-small" data-required="required" data-message="Due date is required" data-plus-as-tab="false" data-clear-type="date" data-fiscal-date="true">
                                    </div>
                                </div>


                                <div class="form-group">
                                    <table id="tbl-sumamry" class="table table-bordered griditem tbl-net-billing align-rights">
                                        <thead>
                                            <tr>
                                                <th colspan="3" class="align-center th-previous">Previous</th>
                                                <th colspan="2" class="align-center th-current">Current Challan</th>
                                                <th colspan="2" class="align-center th-installment">Installemnt</th>
                                            </tr>
                                            <tr>
                                                <th>Total Due</th>
                                                <th>Installemnt Received</th>
                                                <th>Balance</th>
                                                <th>Amount</th>
                                                <th>Balance</th>
                                                <th>Paid</th>
                                                <th>Pending</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr>
                                                <td><span class="TotalAmount"></span></td>
                                                <td><span class="PaidAmount"></span></td>
                                                <td><span class="BalanceAmount"></span></td>
                                                <td><span class="InstallmentAmount"></span></td>
                                                <td><span class="InstallmentBalance"></span></td>
                                                <td>
                                                    <span id="NoOfPaidIstallment">0</span>
                                                </td>
                                                <td>
                                                    <span id="NoOfUnPaidIstallment">0</span>
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>

                                </div>


                                <div class="btn-set pull-right">
                                    @if (!SiteContext.Current.Fiscal.IsClosed)
                                    {

                                        <button type="button" class="btn btn-primary green" onclick="Challans.Save()">
                                            <i class="fa fa-check"></i>
                                            Create Chalaln
                                        </button>

                                    }

                                    <button type="button" class="btn blue" onclick="window.location='@(Url.Content(SettingManager.DashBoardUrl))'">
                                        <i class="fa  fa-times-circle"></i>

                                        Close
                                    </button>
                                </div>

                            </div>

                        </div>
                        <div class="row">
                            <div class="col-md-1"></div>

                            <div class="col-md-8 hide" id="div-table">
                                <div class="portlet box light">
                                    <div class="portlet-title">
                                        <div class="caption">
                                            <i class="fa fa-list"></i>Existing Challans
                                        </div>

                                        <div class="tools">
                                            <a href="javascript:;" class="collapse"></a>
                                        </div>
                                    </div>

                                    <div class="portlet-body">
                                        <div class="hide alert alert-danger container-message">
                                            <i class="close" onclick="$(this).parent().hide();"></i>
                                            <strong></strong>
                                            <div class="form-body message-content"></div>
                                        </div>
                                        <table class="table table-striped table-bordered table-hover" id="mainTable">
                                            <thead>
                                                <tr>

                                                    <th>Challan No.</th>
                                                    <th>Due Date</th>
                                                    <th>Total Amount</th>
                                                    <th>Status</th>
                                                    <th>Rcv. No.</th>
                                                    <th style="width: 150px;" class="no-sort"></th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr>
                                                    <td colspan="5"></td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>



                </div>

            </div>
        </div>
        <!-- END EXAMPLE TABLE PORTLET-->
    </div>
</div>

